  "scripts": {
    "transpile": "rimraf dist && babel --extensions .ts --out-dir dist src",
    "cucumber-compile": "yarn transpile && cucumber-js --config cucumber.js",
    "precucumber": "rimraf reports && mkdir reports && echo {} > reports/report.json",
    "cucumber": "yarn cucumber-compile",
    "postcucumber": "ts-node ./src/reporter/cucumber-report.ts",
    "test:dev": "yarn run cucumber --profile dev || yarn run postcucumber",
    "test": "yarn run cucumber || yarn run postcucumber",
    "test:smoke": "yarn run cucumber --profile smoke --format=junit:reports/report.xml || yarn run postcucumber",
    "test:regression": "yarn run cucumber --profile regression --format=junit:reports/report.xml || yarn run postcucumber"
